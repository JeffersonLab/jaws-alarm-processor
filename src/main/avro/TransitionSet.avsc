{
  "type": "record",
  "name": "TransitionSet",
  "namespace": "org.jlab.jaws.entity",
  "doc": "Set of alarm transitions",
  "fields": [
    {
      "name": "transitionToActive",
      "type": "boolean",
      "default": false,
      "doc": "true when record is first one to indicate Active after being Normal"
    },
    {
      "name": "transitionToNormal",
      "type": "boolean",
      "default": false,
      "doc": "true when record is first one to indicate Normal after being Active"
    },
    {
      "name": "latching",
      "type": "boolean",
      "default": false,
      "doc": "true when record is in-process of latching, LatchAlarm record forthcoming"
    },
    {
      "name": "unshelving",
      "type": "boolean",
      "default": false,
      "doc": "true when record is in-process of being unshelved after one-shot, ShelvedAlarm record clearing"
    }
  ]
}